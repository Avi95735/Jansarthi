  </nav>

  <section id="imageSlider" aria-label="Image slider">
    <div class="slider">
      <div class="carousel">
        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQkf5xJ2Xm3asZ8xg4LwGcynXRkeQ0xE6lYVw&s" style="transform: rotateY(0deg) translateZ(400px);">
        <img src="https://upload.wikimedia.org/wikipedia/en/7/7d/Sdrf_Assam_logo.png" style="transform: rotateY(60deg) translateZ(400px);">
        <img src="https://m.economictimes.com/thumb/msid-88552503,width-1200,height-1200,resizemode-4,imgsize-6916/ngos.jpg" style="transform: rotateY(120deg) translateZ(400px);">
        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRkY7rNABzvT3qn2Yu6iAjBppRyEEmz7eTXqw&s" style="transform: rotateY(180deg) translateZ(400px);">
        <img src="https://upload.wikimedia.org/wikipedia/en/1/13/State_Disaster_Management_Authority_%28Azad_Jammu_%26_Kashmir%29_logo.png" style="transform: rotateY(240deg) translateZ(400px);">
        <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcSG_Ao9AW_LZW5lvJRDUCwBoqqR--fmi3H6Mw&s" style="transform: rotateY(300deg) translateZ(400px);">
        <img src="" style="transform: rotateY(360deg) translateZ(400px);">
        <img src="" style="transform: rotateY(300deg) translateZ(400px);">
      </div>
    </div>
  </section>

